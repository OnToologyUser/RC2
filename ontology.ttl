###  http://www.semanticweb.org/violeta/ontologies/2024/3/airlines-reviews-ontology/review1
ontology:review1 rdf:type owl:NamedIndividual ,
                          ontology:Review ;
                 ontology:hasPassenger ontology:passenger1 ;
                 ontology:hasTrip ontology:trip1 ;
                 ontology:hasComment "Text of review1" ;
                 ontology:hasRating 9 ;
                 ontology:hasTitle "true"^^xsd:boolean ;
                 ontology:isRecommended "Title of review1" .


###  http://www.semanticweb.org/violeta/ontologies/2024/3/airlines-reviews-ontology/ryanair
ontology:ryanair rdf:type owl:NamedIndividual ,
                          ontology:Airlane .


###  http://www.semanticweb.org/violeta/ontologies/2024/3/airlines-reviews-ontology/trip1
ontology:trip1 rdf:type owl:NamedIndividual ,
                        ontology:Trip ;
               ontology:hasAircraft ontology:boeing ;
               ontology:hasAirlaine ontology:ryanair ;
               ontology:isVerified "true"^^xsd:boolean .


